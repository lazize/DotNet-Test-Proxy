<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
    </startup>
    
    <system.net>
        <defaultProxy>
            <proxy proxyaddress="http://my.proxy.server.com:3128"
                   bypassonlocal="True" />
            <bypasslist>
                <add address="[a-z]+\.company\.com" />
            </bypasslist>
        </defaultProxy>
    </system.net>
</configuration>
